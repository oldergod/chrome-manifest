{
  "app": {
    "background": {
      "scripts": ["<%=backgroundjs%>"]
    }
  },
  "author": "<%=author%>",
  "backgroundPage": {
    "background": {
      "page": "background.html",
      "persistent": false
    }
  },
  "backgroundScript": {
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    }
  },
  "browserAction": {
    "browser_action": {
      "default_icon": {
         "19": "<%=icon19%>",
         "38": "<%=icon38%>"
       },
       "default_title": "<%=defaultTitle%>",
       "default_popup": "<%=defaultPopup%>"
     }
  },
  "bluetooth": {
    "uuids": ["<%=bluetoothUUI%&>"],
    "socket": true,
    "low_energy": true
  },
  "manifestVersion": {
    "manifest_version": 2
  },
  "name": {
    "name": "<%=name%>"
  },
  "version": {
    "version": "0.0.0"
  },
  "currentLocale": {
    "current_locale": "<%=defaultLocale%>",
  },
  "defaultLocale": {
    "default_locale": "<%=defaultLocale%>"
  },
  "description": "<%=description%>",
  "icons": {
    "icons": {
      "16": "<%=icon16%>",
      "48": "<%=icon48%>",
      "128": "<%=icon128%>"
    }
  },
  "fileHandlers": {
    "file_handlers": {
      "text": {
        "types": ["text/*"],
        "title": "<%=fileHandlerForText%>"
      },
      "image": {
        "types": [
          "image/png",
          "image/jpeg"
        ],
        "extensions": [
          "tiff"
        ],
        "title": "<%=fileHandlerForImage%>"
      },
      "any": {
        "extensions": [
          "*"
        ],
        "title": "<%=fileHandlerForAny%>"
      }
    }
  },
  "pageAction": {
    "page_action": {
      "default_icon": {
        "19": "<%=icon19%>",
        "38": "<%=icon38%>"
      },
      "default_title": "<%=defaultTitle%>",
      "default_popup": "<%=defaultPopup%>"
    }
  },
  "chromeSettingsOverrides": {
    "chrome_settings_overrides": {
      "homepage": "<%=settingHomepage%>",
      "search_provider": {
        "name": "name.__MSG_url_domain__",
        "keyword": "keyword.__MSG_url_domain__",
        "search_url": "http://www.__MSG_url_domain__/s?q={searchTerms}",
        "favicon_url": "http://www.__MSG_url_domain__/favicon.ico",
        "suggest_url": "http://www.__MSG_url_domain__/suggest?q={searchTerms}",
        "instant_url": "http://www.__MSG_url_domain__/instant?q={searchTerms}",
        "image_url": "http://www.__MSG_url_domain__/image?q={searchTerms}",
        "search_url_post_params": "search_lang=__MSG_url_domain__",
        "suggest_url_post_params": "suggest_lang=__MSG_url_domain__",
        "instant_url_post_params": "instant_lang=__MSG_url_domain__",
        "image_url_post_params": "image_lang=__MSG_url_domain__",
        "alternate_urls": [
          "http://alter1.__MSG_url_domain__/s?q={searchTerms}",
          "http://alter2.__MSG_url_domain__/s?q={searchTerms}"
        ],
        "encoding": "UTF-8",
        "is_default": true
      },
      "startup_pages": ["<%=settingStartupPage%>"]
    },
    "default_locale": "en"
  },
  "commands": {
    "commands": {
      "toggle-feature-x": {
        "suggested_key": {
          "default": "Ctrl+Shift+Y",
          "mac": "Command+Shift+Y"
        },
        "description": "Toggle feature foo"
      },
      "_execute_browser_action": {
        "suggested_key": {
          "windows": "Ctrl+Shift+Y",
          "mac": "Command+Shift+Y",
          "chromeos": "Ctrl+Shift+U",
          "linux": "Ctrl+Shift+J"
        }
      },
      "_execute_page_action": {
        "suggested_key": {
          "default": "Ctrl+Shift+E",
          "windows": "Alt+Shift+P",
          "mac": "Alt+Shift+P"
        }
      }
    }
  },
  "contentScripts": {
    "content_scripts": [
      {
        "matches": ["http://*/*", "https://*/*"],
        "js": ["<%=contentScript"],
        "run_at": "document_end",
        "all_frames": false
      }
    ],
    "permissions": ["tabs", "http://*/*", "https://*/*"]
  },
  "contentSecurityPolicy": {
    "content_security_policy": "<%=cspString%>"
  },
  "devtoolsPage": {
    "minimum_chrome_version": "10.0",
    "devtools_page": "devtools.html"
  },
  "externallyConnectable": {
    "externally_connectable": {
      "ids": ["<%=extConnAppID%>"],
      "matches": ["<%=extConnMatches%>"],
      "accepts_tls_channel_id": false
    }
  },
  "fileBrowserHandlers": {
    "file_browser_handlers": {
      "id": "upload",
      "default_title": "File browser",
      "file_filters": [
        "filesystem:*.*"
      ]
    },
    "icon": {
      "16": "<%=icon16%>"
    },
    "permissions": ["fileBrowserHandler"]
  },
  "homepageUrl": {
    "homepage_url": "<%=homepageURL%>"
  },
  "import": {
    "import": [{"id": "<%=importId%>"}]
  },
  "incognito": {
    "incognito": "spanning"
  },
  "key": {
    "key": "<%=key%>"
  },
  "kioskEnabled": {
    "kiosk_enabled": true
  },
  "kioskOnly": {
    "kiosk_only": true
  },
  "minimumChromeVersion": {
    "minimum_chrome_version": "<%=minimumChromeVersion%>"
  },
  "naclModules": {
    "nacl_modules": [{
      "path": "<%=naclPath%>",
      "mime_type": "<%=naclMimeType%>"
    }]
  },
  "oauth2": {
    "client_id": "<%=oauth2ClientId%>",
    "scopes": ["<%=oauth2Scope%>"]
  },
  "offlineEnabled": {
    "offline_enabled": false
  },
  "omnibox": {
    "omnibox": {
      "keyword": "<%=omniboxKeyword%>"
    },
    "icons": {
      "16": "<%=icon16%>"
    }
  },
  "optionalPermissions": {
    "optional_permissions": ["tabs", "http://*.*"]
  },
  "optionsPage": {
    "options_page": "<%=optionsPage%>"
  },
  "optionsUi": {
    "options_ui": {
      "page": "<%=optionsUIPage%>",
      "chrome_style": true
    }
  },
  "plugins": {
    "plugins": [{"path": "<%=pluginPath%>"}]
  },
  "platforms": {
    "platforms": "<%=platforms%>"
  },
  "requirements": {
    "requirements": {
      "3D": {
        "features": ["webgl"]
      },
      "plugins": {
        "npapi": true
      }
    }
  },
  "sandbox": {
    "sandbox": {
      "pages": ["<%=sandboxPage%>"]
    }
  },
  "shortName": {
    "short_name": "<%=shortName%>"
  },
  "signature": {
    "signature": "<%=signature%>"
  },
  "spellcheck": {
    "spellcheck": "<%=spellcheck%>"
  },
  "storage": {
    "storage": {
      "managed_schema": "<%=storageSchema%>"
    }
  },
  "systemIndicator": {
    "system_indicator": "<%=systemIndicator%>"
  },
  "sockets": {
    "sockets": {
      "udp": {
        "send": ["*:*"],
        "bind": ["*:*"]
      },
      "tcp" : {
        "connect": ["*:*"]
      },
      "tcpServer" : {
        "listen": [":8080"]
      }
    }
  },
  "ttsEngine": {
    "tts_engine": {
      "voices": [{
        "voice_name": "Alice",
        "lang": "en-US",
        "gender": "female",
        "event_types": ["start", "marker", "end"]
      }]
    },
    "permissions": ["ttsEngine"]
  },
  "updateUrl": {
    "update_url": "<%=updateUrl%>"
  },
  "urlHandlers": {
    "url_handlers": {
      "desktop_site": {
        "title": "Desktop Site",
        "matches": [
          "*://example.com/*"
        ]
      },
      "mobile_site": {
        "title": "Mobile Site",
        "matches": [
          "*://m.example.com/*"
        ]
      }
    }
  },
  "versionName": {
    "version_name": "<%=versionName%>"
  },
  "webAccessibleResources": {
    "web_accessible_resources": ["<%=webAccessibleResources%>"]
  },
  "fileSystem": {
    "permissions": [{"fileSystem": ["write", "retainEntries", "directory"]}]
  },
  "mediaGalleries": {
    "permissions": [{"mediaGalleries": ["read", "delete", "copyTo", "allAutoDetected"]}]
  },
  "notifications": {
    "web_accessible_resources": ["<%=icon48%>"],
    "permissions": ["notifications"]
  },
  "contextMenus": {
    "icons": {
      "16": "<%=icon16%>"
    }
  },
  "cookies": {
    "permissions": ["cookies", "*://*.for-cookies.com"]
  },
  "declarativeWebRequest": {
    "permissions": ["declarativeWebRequest", "*://declarative-web-request.com/*"]
  },
  "webRequest": {
    "permissions": ["webRequest", "*://*.web-request.com/"]
  },
  "webview": {
    "webview": {
      "partitions": [
        {
          "name": "static",
          "accessible_resources": ["header.html", "footer.html", "static.png"]
        }
      ]
    },
    "permissions": ["webview"]
  }
}
